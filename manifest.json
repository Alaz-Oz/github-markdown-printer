{
  "manifest_version": 2,
  "name": "GFM to PDF",
  "description": "Convert GitHub Flavored Markdown previews to downloadable PDFs.",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
